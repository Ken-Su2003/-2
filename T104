#include <stdio.h>
#include <stdlib.h>

int main() 
{
     int unit;
     double price=23.34; 
     int total; //double total total*23.24為浮點數 
     printf("請問您要買幾瓶蘋果汁? ");
     scanf("%d", unit); //scanf("%d", &unit);
     total = unit*price;
     printf("我買了%d瓶100%的蘋果汁\n", unit);
     printf("花了%f元", total);  // printf("花了%lf元", total);因為宣告時用double 
     return 0;
}
